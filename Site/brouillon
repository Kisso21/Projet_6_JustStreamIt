                for (let i = 0; i < 6; i++) {
                    console.log("Valeur de i : " + data.results[i].title);
                }
                firstFilmsDiv.innerHTML = `
                    <img src="${allFirstFilm.image_url}" alt="${allFirstFilm.title}">
                    <h2>${allFirstFilm.title} (${allFirstFilm.year})</h2>
                `;
